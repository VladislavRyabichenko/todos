{
  "extends": ["airbnb", "prettier"],
  "plugins": [
    "prettier",
    "react",
    "import"
  ],
  "rules": {
    "no-console": "off",
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "react/prop-types": "off", //!!!
    "no-unused-expressions": 0,
    "no-new" : 0,
    "no-underscore-dangle" : 0,
    "react/jsx-curly-brace-presence": 0,
    "jsx-a11y/anchor-is-valid": 0,
    "jsx-a11y/tabindex-no-positive": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}