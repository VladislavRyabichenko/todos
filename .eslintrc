{
  "extends": ["airbnb", "prettier"],
  "plugins": [
    "prettier",
    "react",
    "import"
  ],
  "rules": {
    "no-console": "off",
    "object-shorthand": 0,
    "no-param-reassign": 0,
    "react/prop-types": "off",
    "no-unused-expressions": 0,
    "no-new" : 0,
    "no-underscore-dangle" : 0,
    "react/jsx-curly-brace-presence": 0,
    "react/jsx-filename-extension": 0,
    "arrow-body-style": 0,
    "react/jsx-no-constructed-context-values": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}